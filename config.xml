  <preference name="WindowsStoreDisplayName" value="World Wine" />
  <vs:platformSpecificValues xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <vs:platformSpecificWidget platformName="windows" id="worldwineMSW" version="0.0.1.0" />
